                "rate": [
                    1, 30
                ], 
                "passes": [
                    "pass=qual quantizer=18 me=esa subme=6 ref=4 interlaced=false cabac=0 threads=0"
                ]
            }, 
            "acodec": {
                "name": "faac", 
                "container": "qtmux", 
                "width": [
                    8, 32
                ], 
                "depth": [
                    8, 24
                ], 
                "rate": [
                    8000, 96000
                ], 
                "channels": [
                    1, 2
                ], 
                "passes": [
                    "bitrate=128000 profile=LC"
                ]
            }
        }
    ]
